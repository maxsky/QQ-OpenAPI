{
    "name": "maxsky/qq-openapi",
    "description": "QQ Open API PHP SDK.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Max Sky",
            "email": "max940213@live.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-sockets": "*",
        "guzzlehttp/guzzle": "^6.5|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Tencent\\QQ\\": "src"
        },
        "files": [
            "src/Constants/ERROR_CODE.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
